<?xml version="1.0" encoding="UTF-8"?>
<conversion>
    <workflow>
        <module name="FalconConvert">
            <with-param name="format" value="MARC21"/>
            <with-param name="script" value="{$p}/check_003.fcv"/>
            <with-param name="output" value="{$f}tmp_1.flc"/>
        </module>
        <module name="FalconConvert">
            <with-param name="input" value="{$f}tmp_1.flc"/>
            <with-param name="format" value="UNIMARC_TAGGED"/>
            <with-param name="script" value="{$p}/check_tags.fcv"/>
            <with-param name="output" value="error_tags_{$f}_{$t}.txt"/>
        </module>
        <module name="FalconIndex">        
            <with-param name="input" value="{$f}tmp_1.flc"/>
            <with-param name="script" value="{$p}/001.ipa"/>
            <with-param name="format" value="UNIMARC_TAGGED"/>
            <with-param name="name" value="001"/>
            <with-param name="die_if_empty" value="true"/>
        </module> 
        <module name="FalconIndex">        
            <with-param name="script" value="{$p}/001.ipa"/>
            <with-param name="format" value="MARC21"/>
            <with-param name="name" value="001"/>
            <with-param name="die_if_empty" value="true"/>
        </module> 
    
        <module name="Copy">
            <with-param name="source" value="{$f}"/>
            <with-param name="target" value="{$f}_copied"/>
        </module>
        <module name="FcvTemplate">
            <with-param name="script" value="test_001_present_unique"/>
            <with-param name="path" value="{$p}"/>
            <with-param name="path_to_file" value="{$ww}copy.mrc" />
        </module>    
        <module name="FcvTemplate">
            <with-param name="script" value="test_001_present_unique2"/>
            <with-param name="path" value="{$p}"/>
            <with-param name="path_to_file" value="{$ww}copy.mrc" />
            <with-param name="format_of_file" value="MARC21" />
        </module>         
    </workflow>
